<div class="card">
	<div class="card-body">
		<h5 class="card-title">
			{{item_desc}}
		</h5>
		<p class="card-text">
			<a href="{{item_url}}" target="_blank">
				<img src="{{item_img_url}}" />
			</a>
		</p>
		<div class="gift-id" style="visibility: hidden;">{{id}}</div>
		<button type="button" class="delete-gift-btn btn btn-danger">Delete Gift</button>
	</div>
</div>